<script type="text/javascript">
	$(document).ready(function () {
		var tbl 		= $('#contenttable'),
			oprList		= {},
			nationList 	= {},
			input_type 	= "add";

		oprList = <%- JSON.stringify(locals.oprList || []) %>;
		nationList = <%- JSON.stringify(locals.nationList || []) %>;
		dataList = <%- JSON.stringify(locals.dataList || []) %>;
		//["STT", "VesselID", "VesselName", "VesselType", "OprID", "NationID", "CallSign",  "Inmarsat", "LLoyd", "IMO", "LOA", "LBP", "MaxBeam", "AntenaHeight", "Depth", "GRT", "DWT"],
		var _columns = [
				{ width: "80px", data: "STT", name: "STT", title: "STT", className: "text-center editor-cancel", targets: 0 },
				{ data: "VesselID", name: "VesselID", title: "Mã tàu", className: "text-center", targets: 1 },
				{ data: "VesselName", name: "VesselName", title: "Tên tàu", className: "text-center", targets: 4 },
				{ data: "VesselType", name: "VesselType", title: "Loại tàu", className: "text-center", targets: 4 },
				{ data: "OprID", name: "OprID", title: "Hãng khai thác", className: "text-center autocomplete", targets: 4 , list: oprList.map(itm=>{return {value:itm.OprID,label:itm.OprName}})},
				{ data: "NationID", name: "NationID", title: "Quốc gia", className: "text-center autocomplete", targets: 4 , list: nationList.map(itm=>{return {value:itm.NationID,label:itm.NationName}})},
				{ data: "CallSign", name: "CallSign", title: "Call Sign", className: "text-center", targets: 4 },
				{ data: "IMO", name: "IMO", title: "IMO", className: "text-center", targets: 4 },
				{ data: "LOA", name: "LOA", title: "LOA", className: "text-center data-type-numeric", targets: 4 },
				{ data: "LBP", name: "LBP", title: "LBP", className: "text-center", targets: 4 },
				{ data: "MaxBeam", name: "MaxBeam", title: "Max Beam", className: "text-center", targets: 4 },
				{ data: "Depth", name: "Depth", title: "Độ sâu", className: "text-center", targets: 4 },
				{ data: "Cellars", name: "Cellars", title: "Số hầm", className: "text-center data-type-numeric", targets: 4 },
				{ data: "GRT", name: "GRT", title: "GRT", className: "text-center", targets: 4 },
				{ data: "DWT", name: "DWT", title: "DWT", className: "text-center", targets: 4 },
				{ data: "rowguid", name: "rowguid", title: "rowguid", visible: false, targets: 6 },
			];
			_columns=_columns.map((itm,ii)=>{return {...itm,targets:ii}});
		var dataTbl = tbl.newDataTable({
			scrollY: '55vh',
			columnDefs: _columns,
			data: dataList.map((itm, ii) => { return { ...itm, STT: ii + 1 } }),
			order: [[0, 'asc']],
			paging: false,
			keys: true,
			autoFill: {
				focus: 'focus',
			},
			select: true,
			rowReorder: false
		});

		if(isMobile.any()){
			$('#vessel-id-list').selectpicker('mobile');
		}

		$('#addrow').on('click', function(){
			$('#inputForm').trigger('reset');
			$('#OprID').selectpicker("refresh");
			$('#NationID').selectpicker("refresh");
			$("#change-vessel-image").val("");
			$("#inputForm").show();
			input_type = "add";
		});

		$('#search').on('click', function () {
			tbl.waitingLoad();
			var btn = $(this);
			btn.button('loading');

			var formData = {
				'action': 'view',
				'filter': {
					'VesselID': $('#VesselIDFilter').val(),
					'VesselName': $('#VesselNameFilter').val()
				}
			};

			$.ajax({
				url: "/Vessel/vsVessel/get",
				dataType: 'json',
				data: formData,
				type: 'POST',
				success: function (data) {
					var rows = [];
					if(data.data.length > 0) {
						for (i = 0; i < data.data.length; i++) {
							let rData = data.data[i];
							rData.STT=i+1;
							rows.push(rData);
						}
					}

					tbl.dataTable().fnClearTable();
		        	if(rows.length > 0){
						tbl.dataTable().fnAddData(rows);
					btn.button('reset');
					}
				},
				error: function(err){
					btn.button('reset');
				}
			});
		});


		$('#save').on('click', function(){
			var VesselID 	= $('#VesselID').val(),
				VesselName	= $('#VesselName').val(),
				VesselType	= $('#VesselType').val(),
				OprID		= $('#OprID').val(),
				NationID	= $('#NationID').val(),
				CallSign	= $('#CallSign').val(),
				Inmarsat	= $('#Inmarsat').val(),
				LLoyd		= $('#LLoyd').val(),
				IMO 		= $('#IMO').val(),
				LOA 		= $('#LOA').val(),
				LBP			= $('#LBP').val(),
				MaxBeam		= $('#MaxBeam').val(),
				AntenaHeight = $('#AntenaHeight').val(),
				Depth		= $('#Depth').val(),
				Cellars		= $('#Cellars').val(),
				GRT			= $('#GRT').val(),
				DWT			= $('#DWT').val();
			
			if (VesselID == ""){
				toastr["error"]('Vui lòng nhập Mã tàu!');	
				return;
			}

			if (VesselName == ""){
				toastr["error"]('Vui lòng nhập Tên tàu!');	
				return;
			}

			if (VesselType == ""){
				toastr["error"]('Vui lòng nhập Loại tàu!');	
				return;
			}

			if (NationID == ""){
				toastr["error"]('Vui lòng chọn Quốc gia!');	
				return;
			}

			if (OprID == ""){
				toastr["error"]('Vui lòng chọn Hãng khai thác!');	
				return;
			}

			if (CallSign == ""){
				toastr["error"]('Vui lòng nhập Call Sign!');	
				return;
			}

			if (IMO == ""){
				toastr["error"]('Vui lòng nhập IMO!');	
				return;
			}


			var vesselArr = [{
				'VesselID': 	VesselID,
				'VesselName': 	VesselName,
				'VesselType': 	VesselType,
				'OprID': 		OprID,
				'NationID': 	NationID,
				'CallSign': 	CallSign,
				'Inmarsat': 	Inmarsat,
				'LLoyd': 		LLoyd,
				'IMO': 			IMO,
				'LOA': 			LOA||null,
				'LBP': 			LBP||null,
				'MaxBeam': 		MaxBeam||null,
				'Depth': 		Depth||null,
				'Cellars': 		Cellars||null,
				'GRT': 			GRT||null,
				'DWT': 			DWT||null,
			}];

			$.confirm({
		            title: 'Thông báo!',
		            type: 'orange',
		            icon: 'fa fa-warning',
		            content: 'Tất cả các thay đổi sẽ được lưu lại!\nTiếp tục?',
		            buttons: {
		                ok: {
		                    text: 'Xác nhận lưu',
		                    btnClass: 'btn-warning',
		                    keys: ['Enter'],
		                    action: function(){
		                        saveData(vesselArr);
		                    }
		                },
		                cancel: {
		                    text: 'Hủy bỏ',
		                    btnClass: 'btn-default',
		                    keys: ['ESC']
		                }
		            }
		    });
		});

		function saveData(formData){
			var fData = {
					'action': input_type,
					'data':  formData,
					'imgSrc': $('#vessel-image').attr('src'),
					'imgName': $('#VesselID').val(),
				};
			postSave(fData);
		}

		function postSave(formData){
			var saveBtn = $('#save');
        	awPost("/Vessel/vsVessel/save",formData).then(data=>{
				if(data.deny) {
					toastr["error"](data.deny);
					return;
				}

				if(data.data){
					toastr["success"]((formData.action == 'edit'?'Cập nhật':'Thêm mới')+" mới thành công!");
					$('#search').trigger('click');
					return;                    	
				}
				else{
					toastr["error"]("Thêm mới thất bại!");
				}
			}).catch((err)=>{
				toastr["error"]("Error!");
                console.log(err);
			})
		}

		// Delete rows
		$('#delete').on('click', function(){
			if(tbl.getSelectedRows().length == 0){
            	$('.toast').remove();
            	toastr["info"]("Vui lòng chọn các dòng dữ liệu để xóa!");
            }
            else{
            	tbl.confirmDelete(function(data){
            		postDel(data);
            	});
            }
		});

		function postDel(data){
			var delData = data;

			var fdel = {
					'action': 'delete',
					'data': delData
				};

			$.ajax({
                url: "/Vessel/vsVessel/delete",
                dataType: 'json',
                data: fdel,
                type: 'POST',
                success: function (data) {
                    if(!data.data) {
                        toastr["error"]('Xóa dữ liệu thất bại!');
                        return;
                    }
                    tbl.DataTable().rows('.selected').remove().draw(false); // Delete row in table
                	tbl.updateSTT(0);
               		toastr["success"]("Xóa dữ liệu thành công!");
                },
                error: function(err){
                	toastr["error"]("Error!");
                	console.log(err);
                }
            });
		}

