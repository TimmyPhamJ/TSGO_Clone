			$('#OprID').selectpicker("refresh");

			$('#NationID').val(NationID);
			$('#NationID').selectpicker("refresh");

			$('#CallSign').val(CallSign);
			$('#Inmarsat').val(Inmarsat);
			$('#LLoyd').val(LLoyd);
			$('#IMO').val(IMO);
			$('#LOA').val(LOA);
			$('#LBP').val(LBP);
			$('#MaxBeam').val(MaxBeam);
			$('#AntenaHeight').val(AntenaHeight);
			$('#Depth').val(Depth);
			$('#Cellars').val(Cellars);
			$('#GRT').val(GRT);
			$('#DWT').val(DWT);

			// Change type from "add" to "edit"
			input_type = 'edit';

			//
			$('#vessel-image').attr("src", "");
