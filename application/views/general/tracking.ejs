		//Thoilc(*Note)-Event key-press
		$('input.search_3').on('keypress', function(e) {
			if (e.keyCode == 13) {
				$('#search').trigger('click');
			}
		});

		//Thoilc(*Note)-Khi người dùng thao tác thông tin tìm kiếm mới show dữ liệu
		function showhiddenSubMain(ishow, opt) {
			if (ishow == "show") {
				if (opt == 'pincode') {
					$(".shadow-box").css({
						"visibility": "visible"
					});
					$("#bb-header").css("display", "none");
					$("#bb-details").css("visibility", "visible");
				} else {
					$(".shadow-box").css({
						"visibility": "visible"
					});
					$("#bb-header").css("display", "block");
					$("#bb-details").css("visibility", "hidden");
				}
			} else if (ishow == "hidden") {
				if (opt == 'pincode') {
					$(".shadow-box").css("visibility", "hidden");
					$("#bb-header").css("display", "none");
					$("#bb-details").css("visibility", "hidden");
				} else {
					$(".shadow-box").css({
						"visibility": "hidden"
					});
					$("#bb-header").css("display", "block");
				}
			}
		}

		//Thoilc(*Note)-Clear dữ liệu khi thay đổi tab
		$('.nav-link').on('click', function(e) {
			if ($(this).attr('data-value') !== $('a.nav-link.active').attr('data-value')) {
				$('.search_3').val("");
			}

			if ($(this).attr('data-value') === 'bill' || $(this).attr('data-value') === 'booking') {
				$("label#cItemName").html("");
				$("label#cCargoTypeName").html("");
				$("label#cClassID").html("");
				$("label#cBlock").html("");
				$("label#cIsLocalForeign").html("");
				$("label#cQuantity").html("");
				$("label#cMcWeight").html("");
				$("label#cVolume").html("");
				$("label#cCusName").html("");
				$("label#cVesselName").html("");
				$("label#cETBD").html("");
				$("label#cGetIn").html("");
				$("label#cGetOut").html("");
				$("label#cJobModeIn").html("");
				$("label#cMethodIn").html("");
				$("label#cJobModeOut").html("");
				$("label#cMethodOut").html("");
