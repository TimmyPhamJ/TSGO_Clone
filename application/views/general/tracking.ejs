				$("#bb-details").css("visibility", "hidden");
			} else {
				$("#bb-header").css("display", "none");
				$("#bb-details").css("visibility", "hidden");
			}
		});

		//Thoilc(*Note)-Load dữ liệu theo thông tin tìm kiếm
		$('#search').on('click', function() {
			if (!$('.search_3').val()) return;
			// $('.content, .cont-error, .bill-error, .booking-error, #bb-details, #cont-details, ul.pagination').addClass('hiden-input');
			$('.header-box').addClass('flex-height');
			$('.search-box').addClass('flex-pdtop');
			$('.ibox-body').removeClass('p-2');
			showhiddenSubMain("show", $('a.nav-link.active').attr('data-value'));
			var dataSend = {
				'opt': $('a.nav-link.active').attr('data-value'),
				'checkval': $('.search_3').val()
			};
			if (dataSend.opt === 'pincode') {
				$.ajax({
					url: "/getPinCode",
					dataType: 'json',
					data: dataSend,
					type: 'POST',
					success: function(dataGet) {
						$("#contenttable").dataTable().fnClearTable();
						let data = dataGet.data;
						if (data.iStatus) {
							let dataAll = data.iPayload;
							toastr["success"](data.iMessage);
							let arr = [];
							dataAll.map(item => {
								arr.push(item);
							});
							$('#contenttable').dataTable().fnAddData(arr);
						} else {
							toastr["info"](data.iMessage);
							$("#contenttable").dataTable().fnClearTable();
						}
					},
					error: function(err) {
						toastr["error"](dataGet.data.iMessage);
						// $('input#search').css('pointer-events', '');
					}
				});
			} else {
				$.ajax({
					url: "/findData",
					dataType: 'json',
					data: dataSend,
					type: 'POST',
					success: function(dataGet) {
						let data = dataGet.data;
						if (data.iStatus) {
							toastr["success"](data.iMessage);
							// $("p#cBBNo").html(data.iPayload[0].BBNo);
							$("label#cItemName").html(data.iPayload[0].ItemName);
							$("label#cCargoTypeName").html(data.iPayload[0].CargoTypeName);
							$("label#cClassID").html(data.iPayload[0].ClassID === 1 ? "Nhập" : "Xuất");
							$("label#cBlock").html(data.iPayload[0].Block);
							$("label#cIsLocalForeign").html(data.iPayload[0].IsLocalForeign === 'L' ? 'Nội' : 'Ngoại');
							$("label#cQuantity").html(data.iPayload[0].Quantity || !data.iPayload[0].Quantity === 0 ? data.iPayload[0].Quantity : '');
							$("label#cMcWeight").html(data.iPayload[0].McWeight || !data.iPayload[0].McWeight === 0 ? data.iPayload[0].McWeight : '');
							$("label#cVolume").html(data.iPayload[0].Volume || !data.iPayload[0].Volume === 0 ? data.iPayload[0].Volume : '');
							$("label#cCusName").html(data.iPayload[0].CusName);
							$("label#cVesselName").html(data.iPayload[0].VesselName + " / " + data.iPayload[0].InboundVoyage + " / " + data.iPayload[0].OutboundVoyage);
							$("label#cETBD").html(moment(data.iPayload[0].ETB).format("YYYY-MM-DD HH:mm:ss") + " / " + moment(data.iPayload[0].ETD).format("YYYY-MM-DD HH:mm:ss"));
							$("label#cGetIn").html(data.iPayload[0].GetIn ? moment(data.iPayload[0].GetIn).format("YYYY-MM-DD HH:mm:ss") : '');
							$("label#cGetOut").html(data.iPayload[0].GetOut ? moment(data.iPayload[0].GetOut).format("YYYY-MM-DD HH:mm:ss") : '');
							$("label#cJobModeIn").html(data.iPayload[0].JobModeNameIN ? data.iPayload[0].JobModeNameIN : '');
							$("label#cMethodIn").html(data.iPayload[0].MethodNameIN ? data.iPayload[0].MethodNameIN : '');
							$("label#cJobModeOut").html(data.iPayload[0].JobModeNameOUT ? data.iPayload[0].JobModeNameOUT : '');
							$("label#cMethodOut").html(data.iPayload[0].MethodNameOUT ? data.iPayload[0].MethodNameOUT : '');
							if (data.iPayload[0].TLHQ) {
								$("li#cTLHQ").html("Đã thanh lý").css("color", "green");
								// $("label#cTLHQ").html("Đã thanh lý").addClass("colorTLHQ text-success");
							} else {
								$("li#cTLHQ").html("Chưa thanh lý").css("color", "red");
								// $("label#cTLHQ").html("Chưa thanh lý").addClass("colorTLHQ text-danger");
							}
						} else {
							toastr["info"](data.iMessage);
							$("label#cItemName").html("");
							$("label#cCargoTypeName").html("");
							$("label#cClassID").html("");
							$("label#cBlock").html("");
							$("label#cIsLocalForeign").html("");
							$("label#cQuantity").html("");
							$("label#cMcWeight").html("");
							$("label#cVolume").html("");
							$("label#cCusName").html("");
							$("label#cVesselName").html("");
							$("label#cETBD").html("");
							$("label#cGetIn").html("");
							$("label#cGetOut").html("");
							$("label#cJobModeIn").html("");
							$("label#cMethodIn").html("");
							$("label#cJobModeOut").html("");
							$("label#cMethodOut").html("");
							$("li#cTLHQ").html("");
						}
					},
					error: function(err) {
						toastr["error"](dataGet.data.iMessage);
						// $('input#search').css('pointer-events', '');
						// console.log(err);
					}
				});
			}
		});

		$(document).on('click', 'ul.pagination li.page-item a', function() {
			var idx = $(this).html();
			if ($(this).hasClass('pre')) {
				idx = parseInt($(this).closest('ul').find('.bg-info').html()) - 1;
			}
			if ($(this).hasClass('next')) {
				idx = parseInt($(this).closest('ul').find('.bg-info').html()) + 1;
			}

			if ($(this).hasClass('bg-info')) return;
			$(this).closest('ul').find('.bg-info').removeClass('bg-info text-white');
			$(this).addClass('bg-info text-white');

			loadContInfo(idx);
		});

		function loadContInfo(idx) {
			if (idx < 0 || idx > _results.length) return;
			var critem = _results[idx - 1];
			if (critem) {
				$.each(critem, function(k, v) {
					$('#cont-details').find('#' + k).html(v);
				});
				$('label#CntrClass').html((critem.CntrClass == '1') ? "Nhập" : ((critem.CntrClass == '2') ? 'Lưu rỗng' : ((critem.CntrClass == '3') ? 'Xuất' : '')));
				$('label#Status').html((critem.Status == 'F') ? "Hàng" : "Rỗng");
				$('label#IsLocal').html((critem.Status == 'F') ? "Ngoại" : "Nội");
				$('label#OOG').html((critem.OOG_TOP == 0 ? "0" : critem.OOG_TOP) + '/' + (critem.OOG_LEFT == 0 ? "0" : critem.OOG_LEFT) + '/' + (critem.OOG_RIGHT == 0 ? "0" : critem.OOG_RIGHT) + '/' +
					(critem.OOG_BACK == 0 ? "0" : critem.OOG_BACK) + '/' + (critem.OOG_FRONT == 0 ? "0" : critem.OOG_FRONT));
				$('label#CMStatus').html((critem.CMStatus == 'S') ? "Stacking" : ((critem.CMStatus == 'D') ? 'Delivery' : ((critem.CMStatus == 'O') ? 'Outgoing' : ((critem.CMStatus == 'I') ? 'Incoming' : ((critem.CMStatus == 'R') ? 'Reserved' : ((critem.CMStatus == 'B') ? 'Baplie Reserved' : 'Remashalling'))))));
				$('label#DateIn').html(getDateTime(critem.DateIn));
				$('label#DateOut').html(getDateTime(critem.DateOut));
			}
		}
	});
</script>
