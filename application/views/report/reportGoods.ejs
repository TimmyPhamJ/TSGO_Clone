<script type="text/javascript">
  $(document).ready(function () {
    $(".load_data").on("click", function () {
      console.log($(".input_direction1").val());
      console.log($(".input_direction2").val());
      console.log($(".input_direction3").val());
      console.log($(".input_direction4").val());
      console.log($(".input_direction5").val());
      console.log($(".input_direction6").val());
      console.log($(".input_direction7").val());

      $.ajax({
        url: "/Report/BaoCao_hanghoa/get-entranceReport",
        dataType: "json",
        type: "POST",
        data: {},
        success: function (data) {
          resolve(data);
        },
        error: function (error) {
          console.log(error);
          reject();
        },
      });
    });
  });
  // end
  $("#TIMEIN").val(moment().add(-7, "day").format("YYYY-MM-DD"));
  $("#TIMEOUT").val(moment().add(7, "day").format("YYYY-MM-DD"));

  $("#TIMEIN,#TIMEOUT").datepicker({
    dateFormat: "yy-mm-dd",
    timeFormat: "hh:mm:ss",
  });
</script>
