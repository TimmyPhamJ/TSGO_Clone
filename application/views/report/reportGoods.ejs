<div>
  <div class="main_report">
    <% %>
    <!-- Header -->

    <div class="header_gate">
      <!-- header tiltle -->
      <div class="header_main">
        <div class="header_title">BÁO CÁO RA VÀO CỔNG</div>
        <div class="wrapp_search">
          <div class="export_excel">
            <span class="btn-icon"><i class="ti-file"></i>Xuất Ecxel</span>
          </div>
          <div class="load_data">
            <span class="btn-icon"><i class="ti-search"></i>Nạp dữ liệu</span>
          </div>
        </div>
      </div>

      <!-- Header content -->
      <div class="header_content">
        <!-- IN OUT -->
        <div class="header_checkbox">
          <div class="check_in">
            <input type="checkbox" />
            <span class="span_checkbox">Vào</span>
          </div>

          <div class="check_out">
            <input type="checkbox" />
            <span class="span_checkbox">Ra</span>
          </div>
        </div>
        <!-- Time -->
        <div class="check_period">
          <div style="display: flex; gap: 10px">
            <span class="span_check-time">Lịch trình tàu</span>
            <div class="row form-group">
              <input
                id="VoyageKey"
                class="form-control form-control-sm input-required"
                type="text"
                hidden
              />

              <input
                class="input_planvessel"
                id="VesselName"
                placeholder="Tên tàu | Chuyến nhập | Chuyến xuất"
                type="text"
              />
              <!-- -->
              <button
                id="chooseVessel"
                class="btn btn-success btn-icon-only btn-circle btn-sm btn-air ml-2"
                style="height: 1.65rem; width: 1.65rem"
                title="Chọn tàu"
              >
                <i class="ti-plus"></i>
              </button>

              <button
                id="nochooseVessel"
                class="btn btn-danger btn-icon-only btn-circle btn-sm btn-air ml-2"
                style="height: 1.65rem; width: 1.65rem"
                title="Bỏ chọn"
              >
                <i class="ti-close"></i>
              </button>
            </div>
          </div>

          <div style="display: flex; gap: 33px">
            <span class="span_check-time">Thời gian</span>
            <div style="display: flex; gap: 20px">
              <div class="">
                <div class="input-group">
                  <input
                    class="form-control form-control-sm input-required width_date"
                    id="TIMEIN"
                    type="text"
                    autocomplete="off"
                    placeholder="Thời gian bắt đầu"
                    value=""
                  />
                </div>
              </div>

              <div class="">
                <div class="input-group">
                  <input
                    class="form-control form-control-sm input-required width_date"
                    id="TIMEIN"
                    type="text"
                    autocomplete="off"
                    placeholder="Thời gian kết thúc"
                    value=""
                  />
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- CÒn lại -->
        <div style="display: flex; gap: 20px">
          <!-- Cột1 -->
          <div style="display: flex; flex-direction: column; gap: 10px">
            <div>
              <span class="text_direction1">Hướng</span>
              <select class="input_direction input_direction1">
                <option class="text_option" value="Hàng nhập">Hàng nhập</option>
                <option value="Hàng xuất">Hàng xuất</option>
              </select>
              <!-- <input class="input_direction" type="" /> -->
            </div>
            <div>
              <span class="text_direction2">Số vận đơn</span>
              <input class="input_direction input_direction2" type="text" />
            </div>
          </div>
          <!--Cột2 -->
          <div style="display: flex; flex-direction: column; gap: 10px">
            <div>
              <span class="text_direction3">Phương án</span>
              <select class="input_direction input_direction3">
                <% result[1].forEach((item) => { %>
                <option value="<%=item.JobModeName  %>">
                  <%= item.JobModeName %>
                </option>
                <% }); %>
              </select>
            </div>
            <div>
              <span class="text_direction4">Số booking</span>
              <input class="input_direction input_direction4" type="text" />
            </div>
          </div>
          <!-- Cột 3 -->
          <div style="display: flex; flex-direction: column; gap: 10px">
            <div>
              <span class="text_direction5">Loại hàng</span>
              <!-- <input class="input_direction" type="text" /> -->
              <select class="input_direction input_direction5">
                <% result[0].forEach((item) => { %>
                <option value="<%=item.ItemName  %>">
                  <%= item.ItemName %>
                </option>
                <% }); %>

                <!-- <option value="Hàng nhập">Hàng nhập</option>
                <option value="Hàng xuất">Hàng xuất</option> -->
              </select>
            </div>
            <div>
              <span class="text_direction6">Số lệnh</span>
              <input class="input_direction input_direction6" type="text" />
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Container -->
    <div class="main_container">
      <div class="container_header">
        <span class="text_container-header">Tìm</span>
        <input class="search_data" type="text" />
      </div>
      <div>
        <span>Số dòng: 0</span>
      </div>
    </div>

    <div class="table_container">
      <div class="wrapp_table">
        <table class="parent_table">
          <tr>
            <th>STT</th>
            <th>Số lệnh</th>
            <th>Lịch trình tàu</th>
            <th>Số vận đơn</th>
            <th>Số Booking</th>
            <th>Hướng</th>
            <th>Loại hàng</th>
            <th>Hàng hóa</th>
            <th>Số lượng</th>
            <th>Trọng lượng</th>
            <th>Vào/ra</th>
            <th>Cổng vào</th>
            <th>Xe vào cổng</th>
            <th>Cổng ra</th>
            <th>Xe ra cổng</th>
            <th>Hàng vào bãi</th>
            <th>Hàng ra bãi</th>
            <th>Số ngày lưu bãi</th>
            <th>Phương tiện</th>
            <th>Số xe/Xà lan</th>
            <th>Số romooc</th>
            <th>Vị trí bãi</th>
            <th>Phương án</th>
            <th>PTGN</th>
            <th>Chủ hàng</th>
            <th>DTTT</th>
            <th>HTTT</th>
            <th>Số hóa đơn</th>
            <th>Số PTC</th>
            <th>Ghi chú</th>
            <th>Cảng chuyển</th>
            <th>Cảng giao nhận</th>
            <th>Ghi chú</th>
          </tr>
          <tr>
            <!-- <td>Alfreds Futterkiste</td>
            <td>Maria Anders</td>
            <td>Germany</td>
            <td>Alfreds Futterkiste</td>
            <td>Maria Anders</td>
            <td>Germany</td>
            <td>Alfreds Futterkiste</td>
            <td>Maria Anders</td>
            <td>Germany</td>
            <td>Alfreds Futterkiste</td>
            <td>Maria Anders</td> -->
          </tr>
        </table>
      </div>
    </div>
  </div>
</div>

